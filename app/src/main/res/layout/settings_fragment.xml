<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical">


    <cse.duytan.coms.customviews.CustomTextView
        app:typeFace="@string/font_nunito_extralight"
        android:id="@+id/textView37"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="30dp"
        android:background="#F3F3F4"
        android:gravity="center_vertical"
        android:paddingLeft="@dimen/padding20"
        style="@style/tvBlack12"
        android:text="@string/lbl_general"
        android:textAllCaps="true"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <cse.duytan.coms.customviews.CustomTextView
        style="@style/tvBlack14"
        android:id="@+id/tvFeshAppData"
        app:typeFace="@string/font_nunito_regular"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="16dp"
        android:text="@string/lbl_fresh_app_data"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView37" />

    <TextView
        android:id="@+id/textView46"
        android:layout_width="0dp"
        android:layout_height="@dimen/line0.5"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="8dp"
        android:background="@color/red"
        app:layout_constraintLeft_toLeftOf="@+id/tvFeshAppData"
        app:layout_constraintRight_toRightOf="@+id/swFreshAppData"
        app:layout_constraintTop_toBottomOf="@+id/tvFeshAppData" />

    <android.support.v7.widget.SwitchCompat
        android:id="@+id/swFreshAppData"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0dp"
        android:layout_marginRight="24dp"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/tvFeshAppData"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tvFeshAppData"
        app:layout_constraintVertical_bias="0.0" />

    <cse.duytan.coms.customviews.CustomTextView
        style="@style/tvBlack14"
        android:id="@+id/tvNotification"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="24dp"
        app:typeFace="@string/font_nunito_regular"
        android:text="@string/lbl_notification"
        app:layout_constraintLeft_toLeftOf="@+id/textView46"
        app:layout_constraintTop_toBottomOf="@+id/textView46" />

    <TextView
        android:id="@+id/textView49"
        android:layout_width="0dp"
        android:layout_height="@dimen/line0.5"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="8dp"
        android:background="@color/red"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@+id/tvNotification"
        app:layout_constraintRight_toRightOf="@+id/swNotification"
        app:layout_constraintTop_toBottomOf="@+id/tvNotification" />

    <android.support.v7.widget.SwitchCompat
        android:id="@+id/swNotification"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0dp"
        android:layout_marginRight="3dp"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/tvNotification"
        app:layout_constraintRight_toRightOf="@+id/textView46"
        app:layout_constraintTop_toTopOf="@+id/tvNotification"
        app:layout_constraintVertical_bias="0.0" />

    <cse.duytan.coms.customviews.CustomTextView
        android:id="@+id/tvEnglish"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="-8dp"
        android:layout_marginTop="24dp"
        style="@style/tvBlack14"
        android:text="@string/lbl_english"
        app:typeFace="@string/font_nunito_regular"
        app:layout_constraintLeft_toLeftOf="@+id/textView49"
        app:layout_constraintTop_toBottomOf="@+id/textView49" />

    <TextView
        android:id="@+id/textView52"
        android:layout_width="0dp"
        android:layout_height="@dimen/line0.5"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="8dp"
        android:background="@color/red"
        app:layout_constraintLeft_toLeftOf="@+id/tvEnglish"
        app:layout_constraintRight_toRightOf="@+id/swEnglish"
        app:layout_constraintTop_toBottomOf="@+id/tvEnglish" />

    <android.support.v7.widget.SwitchCompat
        android:id="@+id/swEnglish"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/tvEnglish"
        app:layout_constraintRight_toRightOf="@+id/textView49"
        app:layout_constraintTop_toTopOf="@+id/tvEnglish" />

    <cse.duytan.coms.customviews.CustomTextView
        app:typeFace="@string/font_nunito_extralight"
        android:id="@+id/textView53"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="23dp"
        android:background="#F3F3F4"
        android:gravity="center_vertical"
        android:paddingLeft="@dimen/padding20"
        android:text="@string/lbl_help_feedback"
        style="@style/tvBlack12"
        android:textAllCaps="true"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView52" />

    <cse.duytan.coms.customviews.CustomTextView
        style="@style/tvBlack14"
        android:id="@+id/tvSupport"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/lbl_support"
        app:typeFace="@string/font_nunito_regular"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/textView53"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/textView52" />

    <TextView
        android:id="@+id/textView55"
        android:layout_width="0dp"
        android:layout_height="@dimen/line0.5"
        android:background="@color/red"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/tvSupport"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/tvSupport"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="@+id/ivSupport"
        app:layout_constraintHorizontal_bias="0.0" />

    <ImageView
        android:id="@+id/ivSupport"
        android:layout_width="15dp"
        android:layout_height="15dp"
        app:srcCompat="@drawable/ic_right_arrow"
        app:layout_constraintTop_toTopOf="@+id/tvSupport"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/tvSupport"
        android:layout_marginBottom="0dp"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="@+id/textView52" />

    <cse.duytan.coms.customviews.CustomTextView
        style="@style/tvBlack14"
        android:id="@+id/tvPrivacyPolicy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/lbl_privacy_policy"
        android:layout_marginTop="24dp"
        app:typeFace="@string/font_nunito_regular"
        app:layout_constraintTop_toBottomOf="@+id/textView55"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/tvSupport" />

    <TextView
        android:id="@+id/textView57"
        android:layout_width="0dp"
        android:layout_height="@dimen/line0.5"
        android:background="@color/red"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/tvPrivacyPolicy"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="@+id/ivPrivacy"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/tvPrivacyPolicy"
        app:layout_constraintHorizontal_bias="0.0" />

    <ImageView
        android:id="@+id/ivPrivacy"
        android:layout_width="15dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/tvPrivacyPolicy"
        app:layout_constraintRight_toRightOf="@+id/textView55"
        app:layout_constraintTop_toTopOf="@+id/tvPrivacyPolicy"
        app:srcCompat="@drawable/ic_right_arrow" />

    <cse.duytan.coms.customviews.CustomTextView
        style="@style/tvBlack14"
        android:id="@+id/tvAbout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/lbl_about_this_app"
        app:typeFace="@string/font_nunito_regular"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/textView57"
        android:layout_marginLeft="-8dp"
        app:layout_constraintLeft_toLeftOf="@+id/textView57" />

    <TextView
        android:id="@+id/textView59"
        android:layout_width="0dp"
        android:layout_height="@dimen/line0.5"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/tvAbout"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/tvAbout"
        android:layout_marginRight="0dp"
        android:background="@color/red"
        app:layout_constraintRight_toRightOf="@+id/ivAbout"
        app:layout_constraintHorizontal_bias="0.0" />

    <ImageView
        android:id="@+id/ivAbout"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginBottom="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/tvAbout"
        app:layout_constraintRight_toRightOf="@+id/textView57"
        app:layout_constraintTop_toTopOf="@+id/tvAbout"
        app:srcCompat="@drawable/ic_right_arrow" />

    <cse.duytan.coms.customviews.CustomTextView
        android:id="@+id/tvVersion"
        style="@style/tvBlack14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="0dp"
        android:text="@string/lbl_version"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView59"
        app:typeFace="@string/font_nunito_regular" />
</android.support.constraint.ConstraintLayout>